@using MvcOnlineOtomasyon.Models.Sınıflar
@model List<KargoDetay>
@{
    ViewBag.Title = "KargoDetay";
    Layout = "~/Views/Shared/_MainView.cshtml";
}
 
<div class="container">
    <br />
    <table class="table table-striped">
        <tr>
            <th>Açıklama</th>
            <th>Tarih</th>
            <th>Takip Kodu</th>
        </tr>
        @foreach (var x in Model)
        {
            <tr>
                <td>@x.Aciklama</td>
                <td>@x.Tarih.ToString("dd-MM-yyyy")</td>
                <td>@x.TakipKodu</td>
            </tr>
        }
    </table>
</div>